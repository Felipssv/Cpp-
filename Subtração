#include <iostream>
int sub(int x, int y) {
    return x - y;
}
int main() {
    int x, y;

    std::cout << "Digite o primeiro número: ";
    std::cin >> x;

    std::cout << "Digite o segundo número: ";
    std::cin >> y;

    int resultado = sub(x, y);

    std::cout << "A subtração é: " << resultado<< std::endl;

    return 0;
}
