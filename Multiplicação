#include <iostream>
float mult(float x, float y) {
    return x * y;
}
int main() {
    float x, y;

    std::cout << "Digite o primeiro número: ";
    std::cin >> x;

    std::cout << "Digite o segundo número: ";
    std::cin >> y;

    float resultado = mult(x, y);

    std::cout << "A multiplicação é: " << resultado<< std::endl;

    return 0;
}
