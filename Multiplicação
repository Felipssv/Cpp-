#include <iostream>
int mult(int x, int y) {
    return x * y;
}
int main() {
    int x, y;

    std::cout << "Digite o primeiro número: ";
    std::cin >> x;

    std::cout << "Digite o segundo número: ";
    std::cin >> y;

    int resultado = mult(x, y);

    std::cout << "A multiplicação é: " << resultado<< std::endl;

    return 0;
}
