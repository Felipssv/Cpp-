#include <iostream>
float soma(float x, float y) {
    return x + y;
}
int main() {
    float x, y;

    std::cout << "Digite o primeiro número: ";
    std::cin >> x;

    std::cout << "Digite o segundo número: ";
    std::cin >> y;

    float resultado = soma(x, y);

    std::cout << "A soma é: " << resultado<< std::endl;

    return 0;
}
